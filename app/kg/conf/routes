# GraphQL
GET         /api/graphql                    controllers.graphql.KgGraphQlController.graphql(query: String, variables: Option[String], operation: Option[String])
POST        /api/graphql                    controllers.graphql.KgGraphQlController.graphqlBody
GET         /api/graphql-schema             controllers.graphql.KgGraphQlController.renderSchema

# Assets
GET         /             controllers.Assets.at(path="/public", file="index.html")
GET         /$path<.*>    controllers.assets.KgAssetsController.frontEndPath(path)
