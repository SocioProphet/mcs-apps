# GraphQL
GET         /api/graphql                    controllers.graphql.KgGraphQlController.graphql(query: String, variables: Option[String], operation: Option[String])
POST        /api/graphql                    controllers.graphql.KgGraphQlController.graphqlBody
GET         /api/graphql-schema             controllers.graphql.KgGraphQlController.renderSchema

# Import
PUT         /api/import/kg/kgtk/edges/:edgesTsvFileName                controllers.import_.KgImportController.putKgtkEdgesTsv(edgesTsvFileName: String)
PUT         /api/import/kg/legacy/edges/:edgesCsvFileName                controllers.import_.KgImportController.putLegacyEdgesCsv(edgesCsvFileName: String)
PUT         /api/import/kg/leagcy/nodes/:nodesCsvFileName                controllers.import_.KgImportController.putLegacyNodesCsv(nodesCsvFileName: String)

# Assets
GET         /             controllers.Assets.at(path="/public", file="index.html")
GET         /$path<.*>    controllers.assets.KgAssetsController.frontEndPath(path)
