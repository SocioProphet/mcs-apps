# GraphQL
GET         /api/graphql                    controllers.graphql.BenchmarkGraphQlController.graphql(query: String, variables: Option[String], operation: Option[String])
POST        /api/graphql                    controllers.graphql.BenchmarkGraphQlController.graphqlBody
GET         /api/graphql-schema             controllers.graphql.BenchmarkGraphQlController.renderSchema

# Assets
GET         /             controllers.Assets.at(path="/public", file="index.html")
GET         /$path<.*>    controllers.test.BenchmarkTestController.frontEndPath(path)
